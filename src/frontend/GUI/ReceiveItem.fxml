<GridPane alignment="BOTTOM_LEFT" hgap="30.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
          minWidth="-Infinity" prefHeight="200.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8"
          xmlns:fx="http://javafx.com/fxml/1" fx:controller="frontend.GUI.ReceiveItemController">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="30.0" prefWidth="100.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="20.0" prefHeight="20.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="50.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="50.0" prefHeight="30.0" vgrow="SOMETIMES"/>
    </rowConstraints>
    <padding>
        <Insets left="50.0" right="50.0" top="50.0"/>
    </padding>
    <children>
        <ChoiceBox fx:id="choiceBox" prefHeight="27.0" prefWidth="109.0" GridPane.rowIndex="1"/>
        <TextField fx:id="tf" maxWidth="90.0" minWidth="20.0" prefHeight="27.0" prefWidth="90.0" promptText="Quantity"
                   GridPane.columnIndex="1" GridPane.rowIndex="1"/>
        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Choose Ingredient"/>
        <Button fx:id="confirmButton" mnemonicParsing="false" onAction="#confirmButtonClicked" text="Confirm"
                GridPane.columnIndex="1" GridPane.rowIndex="2"/>
        <Button mnemonicParsing="false" text="Cancel" GridPane.columnIndex="1" GridPane.halignment="RIGHT"
                GridPane.rowIndex="2" fx:id="cancelButton" onAction="#cancelButtonClicked"/>
    </children>
</GridPane>
